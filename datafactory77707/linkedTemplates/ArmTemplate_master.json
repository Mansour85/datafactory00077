{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory77707"
		},
		"CSV_data_container_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'CSV_data_container'"
		},
		"PoolActivityLog_poolDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'PoolActivityLog_poolDB'"
		},
		"LS_staging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_staging'"
		},
		"CSV_data_container_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake77707.dfs.core.windows.net/"
		},
		"PoolActivityLog_poolDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dataworkspace77707.sql.azuresynapse.net"
		},
		"PoolActivityLog_poolDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "datapool"
		},
		"PoolActivityLog_poolDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory77707_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CSV_data_container_accountKey": {
						"value": "[parameters('CSV_data_container_accountKey')]"
					},
					"PoolActivityLog_poolDB_password": {
						"value": "[parameters('PoolActivityLog_poolDB_password')]"
					},
					"LS_staging_connectionString": {
						"value": "[parameters('LS_staging_connectionString')]"
					},
					"CSV_data_container_properties_typeProperties_url": {
						"value": "[parameters('CSV_data_container_properties_typeProperties_url')]"
					},
					"PoolActivityLog_poolDB_properties_typeProperties_server": {
						"value": "[parameters('PoolActivityLog_poolDB_properties_typeProperties_server')]"
					},
					"PoolActivityLog_poolDB_properties_typeProperties_database": {
						"value": "[parameters('PoolActivityLog_poolDB_properties_typeProperties_database')]"
					},
					"PoolActivityLog_poolDB_properties_typeProperties_userName": {
						"value": "[parameters('PoolActivityLog_poolDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}